import java.util.Scanner;

public class rectangleOfStars {
    public static void repeat(String repstr, int n){
        for (int i = 0; i < n; i++) {
            System.out.print(repstr);
        }
    }
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        for (int i = 0; i < 10; i++) {
          repeat("*", 10);
            System.out.println();
        }
    }
}
